  <group name="Backend">
    <entry name="backend" type="Int">
      <label>Type of backend</label>
      <default code="true">ModelCompilationSettings::getDefaultBackendType()</default>
      <tooltip>0: Sphinx; 1: Julius</tooltip>
    </entry>    
  </group>
</kcfg>

